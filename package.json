{
	"name": "fetch-with-middleware",
	"version": "1.0.0",
	"description": "A lightweight TypeScript library that wraps the Fetch API with support for custom middleware. Zero dependencies, tiny footprint, and easy extensibility.",
	"keywords": [
		"api",
		"browser",
		"fetch",
		"http",
		"middleware",
		"node",
		"request",
		"typescript",
		"wrapper"
	],
	"homepage": "https://github.com/ondrejsevcik/fetch-with-middleware",
	"bugs": {
		"url": "https://github.com/ondrejsevcik/fetch-with-middleware/issues"
	},
	"license": "MIT",
	"author": "Ondrej Sevcik (https://ondrejsevcik.com)",
	"repository": {
		"type": "git",
		"url": "https://github.com/ondrejsevcik/fetch-with-middleware.git"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=10.0.0"
	},
	"main": "./dist/index.js",
	"sideEffects": false,
	"types": "./dist/index.d.ts",
	"files": ["LICENSE.md", "README.md", "dist"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "biome check && npm run lint:ts",
		"lint:fix": "biome check --write",
		"lint:ts": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/node": "^24.3.3",
		"@vitest/coverage-v8": "^3.2.4",
		"semantic-release": "^24.0.0",
		"typescript": "^5.9.2",
		"vite": "^5.3.3",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^3.2.4"
	}
}
