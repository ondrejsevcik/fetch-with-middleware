{
	"name": "fetch-with-middleware",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=10.0.0"
	},
	"files": ["README.md", "LICENSE.md", "dist"],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"homepage": "https://github.com/ondrejsevcik/fetch-with-middleware",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "vite build",
		"preview": "vite preview",
		"lint:ts": "tsc --noEmit",
		"lint": "biome check && npm run lint:ts",
		"lint:fix": "biome check --write",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@lokalise/biome-config": "^1.3.0",
		"@lokalise/package-vite-config": "^3.0.0",
		"@types/node": "^24.3.3",
		"@vitest/coverage-v8": "^3.2.4",
		"semantic-release": "^24.0.0",
		"typescript": "^5.9.2",
		"vite": "^5.3.3",
		"vitest": "^3.2.4"
	}
}
